{
  "version": 2,
  "builds": [
    {
      "src": "src/api/index.ts",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/api/index.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    }
  ],
  "env": {
    "MONGODB_URI": "@mindfulscreen-mongodb-uri",
    "JWT_SECRET": "@mindfulscreen-jwt-secret",
    "JWT_EXPIRE": "@mindfulscreen-jwt-expire",
    "NODE_ENV": "production"
  }
}